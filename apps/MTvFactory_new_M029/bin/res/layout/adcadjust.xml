            android:gravity="center"
            android:text="@string/str_textview_factory_adc_rgain_val"
            android:textColor="#ffffff"
            android:textSize="13dip" />
        <!--
             <ProgressBar android:id="@+id/progressbar_facroty_adc_rgain"
            style="?android:attr/progressBarStyleHorizontal" android:max="2048" android:progress="0"
            android:layout_width="200px" android:layout_height="8px" android:layout_marginTop="23px"
            android:layout_marginLeft="10px" android:layout_below="@+id/progressbar_factory_adc_rgainbelow"
            android:progressDrawable="@drawable/progressbar" />
        -->
