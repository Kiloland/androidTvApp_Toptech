<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="400dip"
    android:layout_height="match_parent"
    android:background="#3280A0"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="47dp"
        android:orientation="vertical" >

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="47dip"
            android:gravity="left"
            android:paddingLeft="20dp"
            android:paddingTop="10dp"
            android:text="@string/str_text_adc_adjust"
            android:textColor="#ffffff"
            android:textSize="17dip" />
    </LinearLayout>
    <!-- For Source -->

    <LinearLayout
        android:id="@+id/linearlayout_factory_adc_source"
        android:layout_width="fill_parent"
        android:layout_height="33dip"
        android:background="@drawable/button_state"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:nextFocusUp="@+id/linearlayout_factory_adc_autoadjust"
        android:orientation="horizontal"
        android:paddingLeft="20dp" >

        <TextView
            android:id="@+id/textview_factory_adc_source"
            android:layout_width="133dip"
            android:layout_height="33dip"
            android:gravity="left|center_vertical"
            android:text="@string/str_factory_adc_source"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <TextView
            android:id="@+id/textview_factory_adc_source_val"
            android:layout_width="133dip"
            android:layout_height="33dip"
            android:gravity="center"
            android:text="@string/str_textview_factory_adc_source_val"
            android:textColor="#ffffff"
            android:textSize="13dip" />
    </LinearLayout>
    <!-- R Gain -->

