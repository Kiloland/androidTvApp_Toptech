        android:focusableInTouchMode="true"
        android:orientation="horizontal"
        android:paddingLeft="20dp" >

        <TextView
            android:id="@+id/textview_sound_nonlinear_osd25"
            android:layout_width="93dip"
            android:layout_height="33dip"
            android:gravity="left|center_vertical"
            android:text="@string/str_factory_nonlinear_osd25"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <TextView
            android:id="@+id/textview_sound_nonlinear_osd25_val"
            android:layout_width="50dip"
            android:layout_height="33dip"
            android:gravity="left|center_vertical"
            android:text="@string/str_textview_factory_nonlinear_osd_val"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <ProgressBar
            android:id="@+id/progressbar_sound_nonlinear_osd25"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="133dip"
            android:layout_height="5dip"
            android:layout_below="@+id/progressbar_sound_nonlinear_osd25below"
            android:layout_marginLeft="7dip"
            android:layout_marginTop="15dip"
            android:max="255"
            android:progress="0"
            android:progressDrawable="@drawable/progressbar" />
    </LinearLayout>
    <!-- For OSD_50 -->

    <LinearLayout
