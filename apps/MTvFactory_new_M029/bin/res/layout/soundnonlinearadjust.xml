<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="400dip"
    android:layout_height="match_parent"
    android:background="#3280A0"
    android:orientation="vertical" >

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="47dp"
        android:gravity="left"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:text="@string/str_factory_non_linear"
        android:textColor="#ffffff"
        android:textSize="17dip" />
    <!-- For Curve Type -->

    <LinearLayout
        android:id="@+id/linearlayout_sound_nonlinear_curvetype"
        android:layout_width="fill_parent"
        android:layout_height="33dip"
        android:background="@drawable/button_state"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:nextFocusUp="@+id/linearlayout_factory_nonlinear_osd100"
        android:orientation="horizontal"
        android:paddingLeft="20dp" >

        <TextView
            android:id="@+id/textview_sound_nonlinear_curvetype"
            android:layout_width="143dip"
            android:layout_height="33dip"
            android:gravity="left|center_vertical"
            android:text="@string/str_factory_nonlinear_curvetype"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <TextView
            android:id="@+id/textview_sound_nonlinear_curvetype_val"
            android:layout_width="133dip"
            android:layout_height="33dip"
            android:gravity="center"
            android:text="@string/str_textview_factory_nonlinear_curvetype_val"
            android:textColor="#ffffff"
            android:textSize="13dip" />
    </LinearLayout>
    <!-- For OSD_0 -->

    <LinearLayout
        android:id="@+id/linearlayout_sound_nonlinear_osd0"
        android:layout_width="fill_parent"
        android:layout_height="33dip"
        android:background="@drawable/button_state"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="horizontal"
        android:paddingLeft="20dp" >

        <TextView
            android:id="@+id/textview_sound_nonlinear_osd0"
            android:layout_width="93dip"
            android:layout_height="33dip"
            android:gravity="left|center_vertical"
            android:text="@string/str_factory_nonlinear_osd0"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <TextView
            android:id="@+id/textview_sound_nonlinear_osd0_val"
            android:layout_width="50dip"
            android:layout_height="33dip"
            android:gravity="left|center_vertical"
            android:text="@string/str_textview_factory_nonlinear_osd_val"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <ProgressBar
            android:id="@+id/progressbar_sound_nonlinear_osd0"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="133dip"
            android:layout_height="5dip"
            android:layout_below="@+id/progressbar_sound_nonlinear_osd0below"
            android:layout_marginLeft="7dip"
            android:layout_marginTop="15dip"
            android:max="255"
            android:progress="0"
            android:progressDrawable="@drawable/progressbar" />
    </LinearLayout>
    <!-- For OSD_25 -->

    <LinearLayout
        android:id="@+id/linearlayout_sound_nonlinear_osd25"
