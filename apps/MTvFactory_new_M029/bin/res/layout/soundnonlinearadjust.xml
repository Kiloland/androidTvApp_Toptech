            android:textSize="13dip" />

        <ProgressBar
            android:id="@+id/progressbar_sound_nonlinear_osd50"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="133dip"
            android:layout_height="5dip"
            android:layout_below="@+id/progressbar_sound_nonlinear_osd50below"
            android:layout_marginLeft="7dip"
            android:layout_marginTop="15dip"
            android:max="255"
            android:progress="0"
            android:progressDrawable="@drawable/progressbar" />
    </LinearLayout>
    <!-- For OSD_75 -->

    <LinearLayout
        android:id="@+id/linearlayout_sound_nonlinear_osd75"
        android:layout_width="fill_parent"
        android:layout_height="33dip"
        android:background="@drawable/button_state"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="horizontal"
        android:paddingLeft="20dp" >

        <TextView
            android:id="@+id/textview_sound_nonlinear_osd75"
            android:layout_width="93dip"
            android:layout_height="33dip"
            android:gravity="left|center_vertical"
            android:text="@string/str_factory_nonlinear_osd75"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <TextView
            android:id="@+id/textview_sound_nonlinear_osd75_val"
            android:layout_width="50dip"
            android:layout_height="33dip"
            android:gravity="left|center_vertical"
            android:text="@string/str_textview_factory_nonlinear_osd_val"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <ProgressBar
            android:id="@+id/progressbar_sound_nonlinear_osd75"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="133dip"
            android:layout_height="5dip"
            android:layout_below="@+id/progressbar_sound_nonlinear_osd75below"
            android:layout_marginLeft="7dip"
            android:layout_marginTop="15dip"
            android:max="255"
            android:progress="0"
            android:progressDrawable="@drawable/progressbar" />
    </LinearLayout>
    <!-- For OSD_100 -->

    <LinearLayout
        android:id="@+id/linearlayout_sound_nonlinear_osd100"
        android:layout_width="fill_parent"
        android:layout_height="33dip"
        android:background="@drawable/button_state"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:nextFocusDown="@+id/linearlayout_sound_nonlinear_curvetype"
        android:orientation="horizontal"
        android:paddingLeft="20dp" >

        <TextView
            android:id="@+id/textview_sound_nonlinear_osd100"
            android:layout_width="93dip"
            android:layout_height="33dip"
            android:gravity="left|center_vertical"
            android:text="@string/str_factory_nonlinear_osd100"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <TextView
            android:id="@+id/textview_sound_nonlinear_osd100_val"
            android:layout_width="50dip"
            android:layout_height="33dip"
            android:gravity="left|center_vertical"
            android:text="@string/str_textview_factory_nonlinear_osd_val"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <ProgressBar
            android:id="@+id/progressbar_sound_nonlinear_osd100"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="133dip"
            android:layout_height="5dip"
            android:layout_below="@+id/progressbar_sound_nonlinear_osd100below"
            android:layout_marginLeft="7dip"
            android:layout_marginTop="15dip"
            android:max="255"
            android:progress="0"
            android:progressDrawable="@drawable/progressbar" />
    </LinearLayout>

</LinearLayout>
