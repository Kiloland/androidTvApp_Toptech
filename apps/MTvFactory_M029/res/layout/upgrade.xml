            android:textSize="13dip" />
    </LinearLayout>
    <!-- For HDCP2 Upgrade -->
    <LinearLayout
        android:id="@+id/linearlayout_factory_hdcp2_upgrade"
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="33dip"
