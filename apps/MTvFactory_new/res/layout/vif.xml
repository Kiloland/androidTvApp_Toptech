            <!-- For VIF_TOP -->

            <LinearLayout
                android:id="@+id/linearlayout_factory_nonstandard_viftop"
                android:layout_width="fill_parent"
                android:layout_height="33dip"
                android:background="@drawable/button_state"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    android:id="@+id/textview_factory_nonstandard_viftop"
                    android:layout_width="180dip"
                    android:layout_height="fill_parent"
                    android:gravity="left|center_vertical"
                    android:text="@string/str_factory_nonstandard_viftop"
                    android:textColor="#ffffff"
                    android:textSize="13dip" />

                <TextView
                    android:id="@+id/textview_factory_nonstandard_viftop_val"
                    android:layout_width="133dip"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="@string/str_textview_factory_nonstandard_default_val"
                    android:textColor="#ffffff"
                    android:textSize="13dip" />
            </LinearLayout>
            <!-- For VIF_AGC_REF -->

            <LinearLayout
                android:id="@+id/linearlayout_factory_nonstandard_vifagcref"
